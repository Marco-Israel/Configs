#include <mi_ooc_encoderDecoder_mock.hpp>


extern "C" {
}



/*******************************************************************************
 **     MACRO definition
 ******************************************************************************/





/*******************************************************************************
 **     Variables and Types EXTERN (public)
 ******************************************************************************/







/*******************************************************************************
 *******************************************************************************
 **                     Globbal MOCKs
 *******************************************************************************
 ******************************************************************************/


/* ----- PART: MOCKs ---------------------------------------------------------*/


extern void ctorEncoderDecoder_MOCK(encoderDecoder__this__st *const ps__this,
        encoderDecoder__ctorParamList__st *const  ps__paramList)
{

    mock().actualCall("ctorEncoderDecoder")
        .withConstPointerParameter("ps__this",ps__this)
        .withConstPointerParameter("ps__paramList",ps__paramList );

}






extern uint8_t encoderDecoder_MOCK(encoderDecoder__this__st *const ps__this,
    uint8_t *const pa__source, uint8_t bytesToSend,
    uint8_t *const pa__destination, uint8_t maxLength),
{

    mock().actualCall("encoderDecoder")
        .withConstPointerParameter("ps__this", ps__this)
        .withConstPointerParameter("pa__source", pa__source)
        .withParameter("bytesToSend", bytesToSend)
        .withConstPointerParameter("pa__destination", pa__destination)
        .withParameter("maxLength", maxLength);

        return 0;

}





extern void encoderDecoderErrorHandler_MOCK(
        encoderDecoder__this__st *const ps__this,
        encoderDecoder__errorHandlerParamList__st *const ps__paramList)
{

    mock().actualCall("encoderDecoderErrorHander")
        .withConstPointerParameter("ps__this", ps__this)
        .withConstPointerParameter("ps__paramList", ps__paramList);

}


/* -------- END PART: MOCKs --------------------------------------------------*/



