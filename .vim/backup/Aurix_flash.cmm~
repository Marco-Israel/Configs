entry &param
goto &param

LoadSymbols_fromfile:
  Data.LOAD.Elf *.elf /NOCODE /SingleLineAdjacent
ENDDO

LoadCode_fromfile:

Program_fromfile:
  SYStem.Mode Down
  SYStem.RESetTarget
  DO ~~/demo/tricore/flash/tc27x.cmm CPU=TC275TP DUALPORT=1 PREPAREONLY
  FLASH.ReProgram ALL
  Data.LOAD * /SingleLineAdjacent
  DO ~~/demo/tricore/flash/tc27x.cmm CPU=TC275TP DUALPORT=1 CHECKBMHD
  ENTRY &bmhdResult
  IF ("&bmhdResult"=="BMHD_OK")
  (
    ; finally program flash memory
    FLASH.ReProgram off
  )
  ELSE
  (
    DIALOG.OK "No valid Boot Mode Header found!" "Reverting loaded data"
    FLASH.ReProgram ALL
    FLASH.ReProgram off
    ENDDO
  )
  SYStem.RESetTarget
ENDDO


