

@startuml

start

:strace32_start.cmm;
:reset_go.men;
:in_target_reset.men;

fork
   :Aurix_menu.cmm
            menuitem
            menuitem;
   :Aurix_flash.cmm;
   fork
       :~~/flash/tc27x.cmm;
    end fork

fork again
   :windows_pos.cmm
        enable Statusbar and Menubar
        open windows and set positon;
end fork

:load History
system.reset
breakpoint load
system <<configuration>>
system.UP;



stop

@enduml
