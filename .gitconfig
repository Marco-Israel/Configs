[user]
	name = Marco-Israel
	email = Marco-Israel@gmx.de
[credential]
	helper = store
[push]
	default = simple
[commit]
  template = ~/.gitfiles/templates/default.tmpl
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
